    .port_info 6 /OUTPUT 4 "count";
P_0x556dbdc047e0 .param/l "WIDTH" 0 3 1, +C4<00000000000000000000000000000100>;
v0x556dbdbc8dc0_0 .net "clk", 0 0, v0x556dbdc1af00_0;  1 drivers
v0x556dbdc1a900_0 .var "count", 3 0;
