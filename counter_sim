v0x556dbdc1ad60_0 .net "up", 0 0, v0x556dbdc1b420_0;  1 drivers
E_0x556dbdc01ba0/0 .event negedge, v0x556dbdc1aca0_0;
E_0x556dbdc01ba0/1 .event posedge, v0x556dbdbc8dc0_0;
E_0x556dbdc01ba0 .event/or E_0x556dbdc01ba0/0, E_0x556dbdc01ba0/1;
    .scope S_0x556dbdc04600;
T_0 ;
    %wait E_0x556dbdc01ba0;
    %load/vec4 v0x556dbdc1aca0_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x556dbdc1a900_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x556dbdc1aab0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
    %load/vec4 v0x556dbdc1ab70_0;
    %assign/vec4 v0x556dbdc1a900_0, 0;
    %jmp T_0.3;
T_0.2 ;
    %load/vec4 v0x556dbdc1a9e0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.4, 8;
    %load/vec4 v0x556dbdc1ad60_0;
    %flag_set/vec4 8;
    %jmp/0 T_0.6, 8;
    %load/vec4 v0x556dbdc1a900_0;
    %addi 1, 0, 4;
    %jmp/1 T_0.7, 8;
T_0.6 ; End of true expr.
    %load/vec4 v0x556dbdc1a900_0;
    %subi 1, 0, 4;
    %jmp/0 T_0.7, 8;
 ; End of false expr.
    %blend;
T_0.7;
    %assign/vec4 v0x556dbdc1a900_0, 0;
T_0.4 ;
T_0.3 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x556dbdc04470;
T_1 ;
    %delay 5, 0;
    %load/vec4 v0x556dbdc1af00_0;
    %inv;
    %store/vec4 v0x556dbdc1af00_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x556dbdc04470;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x556dbdc1af00_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x556dbdc1b350_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x556dbdc1b090_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x556dbdc1b420_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x556dbdc1b190_0, 0, 1;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x556dbdc1b260_0, 0, 4;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x556dbdc1b350_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x556dbdc1b190_0, 0, 1;
    %pushi/vec4 10, 0, 4;
    %store/vec4 v0x556dbdc1b260_0, 0, 4;
    %delay 10, 0;
